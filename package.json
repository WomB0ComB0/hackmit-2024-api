{
  "name": "sparkmind-scraper-api",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch server --watch server.ts --exec tsx server.ts",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "lint": "bunx biome check --write ./",
    "preview": "vite preview",
    "build:client": "vite build --outDir dist/app/client",
    "build:server": "tsc -p tsconfig.server.json && vite build --ssr src/entry-server.tsx --outDir dist/entry",
    "clean": "rimraf dist",
    "serve": "cross-env NODE_ENV=production node ./dist/app/server",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "scraper",
    "api",
    "express",
    "puppeteer",
    "react",
    "react-router-dom"
  ],
  "author": "Mike Odnis",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/serve-static": "^1.15.7",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "puppeteer": "^22.15.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "supertest": "^7.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "compression": "^1.7.4",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  }
}
